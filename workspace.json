{
  "version": 2,
  "projects": {
    "1-tech-arch-myapi": {
      "root": "apps/1-tech-arch/myapi",
      "sourceRoot": "apps/1-tech-arch/myapi/src",
      "projectType": "application",
      "prefix": "1-tech-arch-myapi",
      "generators": {},
      "targets": {
        "build": {
          "executor": "@flowaccount/nx-serverless:build",
          "options": {
            "outputPath": "dist/apps/1-tech-arch/myapi",
            "package": "apps/1-tech-arch/myapi",
            "serverlessConfig": "apps/1-tech-arch/myapi/serverless.yml",
            "servicePath": "apps/1-tech-arch/myapi",
            "tsConfig": "apps/1-tech-arch/myapi/tsconfig.app.json",
            "provider": "aws",
            "processEnvironmentFile": "env.json"
          },
          "configurations": {
            "dev": {
              "optimization": false,
              "sourceMap": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            },
            "production": {
              "optimization": true,
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/1-tech-arch/myapi/environment.ts",
                  "with": "apps/1-tech-arch/myapi/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@flowaccount/nx-serverless:offline",
          "options": {
            "buildTarget": "1-tech-arch-myapi:build",
            "config": "apps/1-tech-arch/myapi/serverless.yml",
            "location": "dist/apps/1-tech-arch/myapi",
            "port": 7777
          },
          "configurations": {
            "dev": {
              "buildTarget": "1-tech-arch-myapi:build:dev"
            },
            "production": {
              "buildTarget": "1-tech-arch-myapi:build:production"
            }
          }
        },
        "deploy": {
          "executor": "@flowaccount/nx-serverless:deploy",
          "options": {
            "buildTarget": "1-tech-arch-myapi:build:production",
            "config": "apps/1-tech-arch/myapi/serverless.yml",
            "location": "dist/apps/1-tech-arch/myapi",
            "package": "dist/apps/1-tech-arch/myapi",
            "stage": "dev"
          }
        },
        "destroy": {
          "executor": "@flowaccount/nx-serverless:destroy",
          "options": {
            "buildTarget": "1-tech-arch-myapi:build:production",
            "config": "apps/1-tech-arch/myapi/serverless.yml",
            "location": "dist/apps/1-tech-arch/myapi",
            "package": "dist/apps/1-tech-arch/myapi"
          }
        },
        "lint": {
          "executor": "@angular-devkit/build-angular:tslint",
          "options": {
            "exclude": [
              "**/node_modules/**",
              "!apps/1-tech-arch/myapi/**/*"
            ],
            "tsConfig": [
              "apps/1-tech-arch/myapi/tsconfig.spec.json"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/1-tech-arch/myapi"
          ],
          "options": {
            "jestConfig": "apps/1-tech-arch/myapi/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/workspace"
  }
}
